<table class="table table-striped table-bordered">
  <thead>
    <tr><h3>Breach at <%= human_time_of_day(breach.time) %></h3> </tr>
    <tr>
        <!-- <b>Type:</b>   <%# breach.code_type.andand.name %> --  -->  
        <b>Codes:</b> <% breach.codes.each do |code| %>
            <%= code.name %>, 

            <% end %>
        
    </tr>
    
    <tr>
        <th>Person</th>
        <th>Contribution</th>
    </tr>
</thead>
<tbody>
    <% breach.contributions.each do |contribution| %>
    <tr>
        <td><a href="<%= person_url(contribution.person) %>"><%= contribution.person.name %></a></td>
        <td><%= contribution.text %></td>
    </tr>

    <% end %>
</tbody>
</table>
<div>
    <!-- Do this for the whole session -->
    <%# render partial: "breaches/stats", locals: {breach: breach} %>
</div>
