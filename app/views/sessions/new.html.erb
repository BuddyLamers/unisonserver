<%= form_tag sessions_path do %>

	<fieldset>
    <%= label_tag :email %>
    <%= text_field_tag :email, params[:email] %>
  </fieldset>

  <fieldset>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </fieldset>

	<fieldset>
		<%= submit_tag "Login", class: "btn" %>
	</fieldset>
<% end %>
